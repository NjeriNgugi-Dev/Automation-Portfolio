    {
      "parameters": {
        "jsCode": "// Clean and structure developer data\nconst profileData = $input.item.json;\n\n// Calculate account age\nconst accountAge = profileData.created_at \n  ? ((new Date() - new Date(profileData.created_at)) / (1000 * 60 * 60 * 24 * 365)).toFixed(1)\n  : 0;\n\nconst developer = {\n  json: {\n    // Personal Info\n    name: profileData.name || profileData.login,\n    username: profileData.login,\n    email: profileData.email || '',\n    bio: profileData.bio || '',\n    \n    // Location\n    location: profileData.location || '',\n    company: profileData.company || '',\n    \n    // Links\n    githubUrl: profileData.html_url,\n    websiteUrl: profileData.blog || '',\n    twitterUsername: profileData.twitter_username || '',\n    avatarUrl: profileData.avatar_url,\n    \n    // Stats\n    publicRepos: profileData.public_repos || 0,\n    followers: profileData.followers || 0,\n    following: profileData.following || 0,\n    \n    // Status\n    hireable: profileData.hireable || false,\n    accountAgeYears: accountAge,\n    \n    // Meta\n    platform: 'GitHub',\n    scrapedDate: new Date().toISOString()\n  }\n};\n\nreturn developer;"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
