      "id": "6bcb4924-0036-46fd-a498-c7b0528a30fa",
      "name": "Clean and Structure Data"
    },
    {
      "parameters": {
        "jsCode": "// Score each developer as a lead\nconst dev = $input.item.json;\n\nlet score = 0;\n\n// Scoring system\nif (dev.email) score += 15;\nif (dev.followers > 100) score += 20;\nelse if (dev.followers > 50) score += 15;\nelse if (dev.followers > 20) score += 10;\nelse if (dev.followers > 10) score += 5;\n\nif (dev.publicRepos > 50) score += 20;\nelse if (dev.publicRepos > 20) score += 15;\nelse if (dev.publicRepos > 10) score += 10;\nelse if (dev.publicRepos > 5) score += 5;\n\nif (dev.bio && dev.bio.length > 20) score += 10;\nif (dev.websiteUrl) score += 10;\nif (dev.hireable) score += 15;\n\nif (dev.accountAgeYears > 3) score += 10;\nelse if (dev.accountAgeYears > 1) score += 5;\n\n// Determine quality\nlet leadQuality = 'Low';\nif (score >= 70) leadQuality = 'High';\nelse if (score >= 50) leadQuality = 'Medium';\n\n// Determine experience\nlet experienceLevel = 'Junior';\nif (dev.publicRepos > 30 && dev.followers > 50 && dev.accountAgeYears > 2) {\n  experienceLevel = 'Senior';\n} else if (dev.publicRepos > 15 && dev.followers > 20) {\n  experienceLevel = 'Mid-Level';\n}\n\nreturn {\n  json: {\n    ...dev,\n    leadScore: score,\n    leadQuality: leadQuality,\n    experienceLevel: experienceLevel,\n    hasContactInfo: !!(dev.email || dev.websiteUrl || dev.twitterUsername),\n    readyToContact: !!(dev.email && dev.hireable)\n  }\n};\n"
      },
